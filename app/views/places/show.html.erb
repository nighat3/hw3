<h1><%= @place["name"] %></h1>

<h2>Journal Entries</h2>
<ul>
 <% for entry in @entries %>
    <li>
    <p>
      <%= entry["title"] %>
      </p>
      <p>
      <%= entry["occurred_on"] %>
      </p>
      <p>
      <%= entry["description"] %>
      </p>
    </li>
</ul>
<%end%>

<p><a href="/entries/new?place_id=<%=params["id"]%>">Add a new Entry</a></p>